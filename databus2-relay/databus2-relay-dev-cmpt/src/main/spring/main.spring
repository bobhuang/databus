        <ref bean="databus2Relay.liar"/>
        <ref bean="databus2Relay.member2"/>
        <ref bean="databus2Relay.anet"/>
        <ref bean="databus2Relay.conn"/>
        <ref bean="databus2Relay.mbrrec"/>
        <ref bean="databus2Relay.news"/>
      </list>
    </property>
  </lin:bean>
  
</beans>
