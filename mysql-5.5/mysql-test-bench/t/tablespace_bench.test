--source include/have_innodb.inc
set global innodb_file_format='Barracuda';
let $TARGET_DIR=$MYSQLTEST_VARDIR/tmp/xtrabackup;
let $MYSQLTEST_DATADIR=`select @@datadir`;

--source suite/li_bench/t/tablespace_bench_slave.inc
--exec mkdir -p $TARGET_DIR
--exec if [ -n "$SRC_DIR" ] ; then cp -r $SRC_DIR/* $TARGET_DIR ; fi

--exec echo 'starting multi-table-bench' >> $OUTPUT_DIR/$OUTPUT_FILE

--exec $MYSQL_TEST_DIR/li-utils/multi-table-bench -S $MASTER_MYSOCK --num-threads=$NUM_THREADS --num-tables=$NUM_TABLES --num-dbs=$NUM_DBS --min-key=1 --max-key=$NUM_ROWS --num-rounds=$NUM_ROUNDS --blob-len=$BLOB_LEN --table-opts='$TABLE_OPTS' --num-secondary-keys=$NUM_SECONDARY_KEYS --num-joins=$NUM_JOINS --target-dir=$TARGET_DIR --sleep-between-queries=$SLEEP_BETWEEN_QUERIES --pre-warm=$PRE_WARM --load-ratios=$LOAD_RATIOS 2>&1 >> $OUTPUT_DIR/$OUTPUT_FILE

--exec echo 'finished multi-table-bench' >> $OUTPUT_DIR/$OUTPUT_FILE

set global innodb_file_format='Antelope';
