source include/master-slave.inc;
connection master;
create table test.t1(id int not null primary key, val blob not null) 
  engine=myisam;
--exec  $MYSQL_TEST_DIR/suite/rpl_dbus/scripts/li_socket_client.py --port=8999 --skip-print
select count(*),min(id),max(id) from test.t1;
select id,val,length(val) from test.t1 limit 20;
sync_slave_with_master;
connection slave;
select count(*),min(id),max(id) from test.t1;
select id,val,length(val) from test.t1 limit 20;
connection master;
drop table test.t1;
sync_slave_with_master;
