                          {"value":"INT","label":"Internet"},
                          {"value":"LEG","label":"Legal"},
                          {"value":"MAN","label":"Manufacturing"},
                          {"value":"MARKET","label":"Marketing & Advertising"},
                          {"value":"NON","label":"Non Profit"},
                          {"value":"PACK","label":"Packaged Goods"},
                          {"value":"PHARM","label":"Pharmaceutical"},
                          {"value":"PUB","label":"Publishing & Media"},
                          {"value":"REAL","label":"Real Estate"},
                          {"value":"RECRUIT","label":"Recruitment"},
                          {"value":"RET","label":"Retail"},
                          {"value":"TECH","label":"Technology"},
                          {"value":"TELE","label":"Telecommunications"},
                          {"value":"TRAVEL","label":"Travel"},
                          {"value":"WEB","label":"Web Design & Development"},
                          {"value":"WRITING","label":"Writing & Translation"},
                          {"value":"OTHER","label":"Other"}]}]},
    {"name": "description",
     "type": ["null",
              {"name": "BizText",
               "type": "record",
               "fields": [{"name": "isRichText", "type": "boolean"},
