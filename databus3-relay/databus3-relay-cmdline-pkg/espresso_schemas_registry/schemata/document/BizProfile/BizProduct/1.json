                          {"value":"BIZ","label":"Business Services"},
                          {"value":"CONSULT","label":"Consulting"},
                          {"value":"CONS","label":"Consumer Electronics"},
                          {"value":"EDU","label":"Education"},
                          {"value":"ENER","label":"Energy Services"},
                          {"value":"ENV","label":"Environmental Services"},
                          {"value":"LEGAL","label":"Financial Services"},
                          {"value":"GOV","label":"Government"},
                          {"value":"HEALTH","label":"Health & Well Being"},
